<% content_for :title, "Catálogos" %>  

<div class="row-fluid">
  <div class="page-header">
     <h1><%= @programmer.first_name %></h1>
  </div>

  <p>
    <b>Nombre:</b>
    <%= @programmer.first_name %>
  </p>
  
  <p>
    <b>Apellidos:</b>
    <%= @programmer.last_name %>
  </p>
  
  <p>
    <b>Email:</b>
    <%= @programmer.email %>
  </p>

  <p>
    <b>Creación:</b>
    <%= l @programmer.created_at, :format => :date_time %>
  </p>

  <p>
    <b>Última actualización:</b>
    <%= l @programmer.updated_at, :format => :date_time %>
  </p>	

  <div class="form-actions">
    <%= link_to 'Editar',edit_management_programmer_path(@programmer), { :class => "btn btn-primary" } %> 
  </div>
</div>

