<% content_for :title, "Catálogos" %> 
<div class="row-fluid">
  <div class="page-header">
          <h1><%= @diagram.name %></h1>
  </div>

  <p>
    <b>Nombre:</b>
    <%= @diagram.name %>
  </p>

  <p>
    <b>Creación:</b>
    <%= l @diagram.created_at,  :format => :date_time %>
  </p>

  <p>
    <b>Última actualización:</b>
    <%= l @diagram.updated_at, :format => :date_time %>
  </p>	

  <div class="form-actions">
    <%= link_to 'Editar', edit_diagram_path(@diagram), { :class => "btn btn-primary" } %> 
  </div>
</div>


